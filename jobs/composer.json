{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "version": "14.0.0-patch-2",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laracasts/flash": "^3.2",
        "laravel/cashier": "^14.7",
        "laravel/framework": "^10.17",
        "laravel/helpers": "^1.6",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.11",
        "maatwebsite/excel": "^3.1",
        "magarrent/laravel-currency-formatter": "^1.0",
        "mariuzzo/laravel-js-localization": "^1.10",
        "opcodesio/log-viewer": "^1.9",
        "pragmarx/countries": "^0.7.0",
        "rappasoft/laravel-livewire-tables": "^2.11",
        "rennokki/laravel-eloquent-query-cache": "^3.4",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "^5.8",
        "spatie/schema-org": "^3.11",
        "srmklive/paypal": "^3.0",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "beyondcode/laravel-query-detector": "^1.7",
        "imanghafoori/laravel-microscope": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^9.5.10",
        "fakerphp/faker": "^1.9.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
